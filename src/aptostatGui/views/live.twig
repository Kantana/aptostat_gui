<div class="front_messagebox">
    {% if not realTime %}
        Could not connect to live
    {% else %}
        <ol class="tree">
            {% for serviceGroupName, serviceGroup in realTime %}
                <li>
                    {% if (serviceGroup['groupState'] == 'up') %}
                        <label for='{{ serviceGroupName }}'>
                            <strong>{{ serviceGroupName }}</strong>
                            <div class="pull-right"><img src='../img/check.png' /> <strong>OK</strong></div>
                        </label>
                        <input type='checkbox' id='{{ serviceGroupName }}' />
                    {% elseif (serviceGroup['groupState']) == 'warning' %}
                        <label for='{{ serviceGroupName }}'>
                            <strong>{{ serviceGroupName }}</strong>
                            <div class="pull-right"><img src='../img/warning.png' /> <strong>Problem</strong></div>
                        </label>
                        <input type='checkbox' checked id='{{ serviceGroupName }}' />
                    {% else %}
                        <label for='{{ serviceGroupName }}'>
                            <strong>{{ serviceGroupName }}</strong>
                            <div class="pull-right"><img src='../img/cross.png' /> <strong>Down</strong></div>
                        </label>
                        <input type='checkbox' checked id='{{ serviceGroupName }}' />
                    {% endif %}

                    <ol>
                        {% for service, state in serviceGroup if service != 'groupState' %}
                            <li class="file">
                                {{ service }}
                                {% if state == 'up' %}
                                    <div class="pull-right"><img src='../img/check.png' /> OK</div>
                                {% elseif state == 'warning' %}
                                    <div class="pull-right"><img src='../img/warning.png' /> Problem</div>
                                {% else %}
                                    <div class="pull-right"><img src='../img/cross.png' /> Down</div>
                                {% endif %}
                            </li>
                        {% endfor %}
                    </ol>
                </li>
            {% endfor %}
        </ol>
    {% endif %}
</div>